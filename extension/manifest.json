    "content_scripts": [
        {
            "matches": [
                "https://kite.zerodha.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "OHLC Viewer"
    }
}